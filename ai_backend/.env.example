# AI Workflow Service Environment Variables

# OpenAI API Key (recommended for best results)
OPENAI_API_KEY=

# Alternative: Together AI (for open-source models)
TOGETHER_API_KEY=

# Alternative: OpenRouter (for various models)
OPENROUTER_API_KEY=

# Service Port (default: 8000)
AI_SERVICE_PORT=8000

# Google Sheets credentials path (optional)
GOOGLE_SHEETS_CREDENTIALS=

# RAG documents directory (optional)
RAG_DOCUMENTS_PATH=./documents
